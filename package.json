    "hot-module-replacement",
    "eslint",
    "eslint-loader",
    "stylelint",
    "html-webpack-plugin"
  ],
  "homepage": "https://github.com/SimoNonnis/react-filter-sort#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimoNonnis/react-filter-sort"
  },
  "bugs": {
    "url": "https://github.com/SimoNonnis/react-filter-sort/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
